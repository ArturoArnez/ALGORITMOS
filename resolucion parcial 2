bool primo(int N) //porque mary dijo
{ 
	bool primo=true;

   for (int i=2;i<N;i++)

   { if(N%1==0)


   { primo=false;
   }

   } return primo;
}


void ordenarprimo(vector mat[][],int n,int m)
{ 
	int Vp[],tamaño;


	for (int i=0;i<N;i++)

	{
		for (int j=0;j<N;J++)
	 
		{ tamaño=0;

		for (int k;k<mat[i][j].tamaño;k++)

			if(primo(mat[i][j].vec[k]==true))
			{ Vp[tamaño]=mat[i][j].vec[k];
		      tamaño++;

		    }
		}
		ordenar(Vp,tamaño);
		for (int k=mat[i][j].tam-1;k<=0;k++)
		{ if (primo(mat[i][j].vec[k]=Vp[tamaño])==true)

		  tamaño--;
		matriz[i][j].vec[k]=Vp[tamaño];

